{
  "name": "my-vcard",
  "version": "2.0.0",
  "private": true,
  "description": "Ricardo Sousa's vCard",
  "author": "Ricardo Sousa",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prestart:storybook": "cp -r ./public/page-data/sq/d ./public/static",
    "start:storybook": "NODE_ENV=test start-storybook -p 6006 --no-dll -s public",
    "start:gatsby": "gatsby develop -H 0.0.0.0",
    "clean": "gatsby clean",
    "preci": "rm -rf node_modules",
    "ci": "yarn install --immutable --immutable-cache --check-cache",
    "prebuild:storybook": "yarn prestorybook",
    "build:storybook": "NODE_ENV=test build-storybook -c .storybook -o ./public/storybook",
    "build:gatsby": "GATSBY_BUILD_TIME=`date +'%Y-%m-%dT%H:%M:%S%z'` gatsby build --prefix-paths",
    "build": "yarn clean && yarn build:gatsby && yarn build:storybook",
    "serve": "gatsby serve --prefix-paths -H 0.0.0.0",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "format": "npx prettier --write .",
    "trans:verify": "node scripts/verify-translations.js",
    "trans:extract": "node scripts/extract-translations.js",
    "hooks": "npx lefthook run pre-commit -v"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/over-the-rainbow": "^4.5.10",
    "@fontsource/poppins": "^4.5.10",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@reach/router": "^1.3.4",
    "browser-monads-ts": "^2.0.1",
    "date-fns": "^2.29.3",
    "fetch-jsonp": "^1.2.3",
    "formik": "^2.2.9",
    "gatsby": "^4.24.4",
    "gatsby-background-image": "^1.6.0",
    "gatsby-plugin-emotion": "^7.24.0",
    "gatsby-plugin-image": "^2.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-react-helmet": "^5.24.0",
    "gatsby-plugin-react-i18next": "^2.0.4",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-sitemap": "^5.24.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-theme-material-ui": "^5.3.0",
    "gatsby-transformer-remark": "^5.24.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.8",
    "i18next-http-backend": "^1.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.18.6",
    "react-use": "^17.4.0",
    "typewriter-effect": "^2.19.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@a110/storybook-expand-all": "^1.1.1",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-typescript": "^7.19.3",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-storysource": "^6.5.12",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/builder-webpack4": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack4": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/gatsbyjs__reach-router": "^1.3.0",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-i18next-extract": "^0.9.0",
    "babel-preset-gatsby": "^3.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-storybook": "^0.6.6",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.27",
    "lefthook": "^1.1.2",
    "prettier": "^2.7.1",
    "storybook-addon-gatsby": "^0.0.5",
    "storybook-dark-mode": "^1.1.2",
    "storybook-react-i18next": "^1.1.2",
    "typescript": "^4.8.4",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "yarn-upgrade-all": "^0.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ricardomfmsousa/my-vcard"
  }
}
