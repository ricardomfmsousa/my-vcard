{
  "name": "my-vcard",
  "version": "2.0.0",
  "private": true,
  "description": "Ricardo Sousa's vCard",
  "author": "Ricardo Sousa",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "preci": "rm -rf node_modules",
    "ci": "yarn install --immutable --immutable-cache --check-cache",
    "prestorybook": "cp -r ./public/page-data/sq/d ./public/static",
    "storybook": "NODE_ENV=test start-storybook -p 6006 --no-dll -s public",
    "start": "gatsby develop -H 0.0.0.0",
    "clean": "gatsby clean",
    "build:gatsby": "gatsby build --prefix-paths",
    "prebuild:storybook": "yarn prestorybook",
    "build:storybook": "NODE_ENV=test build-storybook -c .storybook -o ./public/storybook",
    "build": "yarn clean && yarn build:gatsby && yarn build:storybook",
    "serve": "gatsby serve --prefix-paths -H 0.0.0.0",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "format": "npx prettier --write .",
    "trans:verify": "node scripts/verify-translations.js",
    "trans:extract": "yarn run babel --config-file ./scripts/extract-translations.js -o tmp/chunk.js './src/**/*.{js,jsx,ts,tsx}' && rm -rf tmp"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/over-the-rainbow": "^4.5.9",
    "@fontsource/poppins": "^4.5.9",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.1",
    "date-fns": "^2.29.2",
    "fetch-jsonp": "^1.2.2",
    "formik": "^2.2.9",
    "gatsby": "^4.21.0",
    "gatsby-background-image": "^1.6.0",
    "gatsby-plugin-emotion": "^7.21.0",
    "gatsby-plugin-image": "^2.21.0",
    "gatsby-plugin-manifest": "^4.21.0",
    "gatsby-plugin-react-i18next": "^2.0.4",
    "gatsby-plugin-sharp": "^4.21.0",
    "gatsby-plugin-sitemap": "^5.21.0",
    "gatsby-source-filesystem": "^4.21.1",
    "gatsby-theme-material-ui": "^5.3.0",
    "gatsby-transformer-remark": "^5.21.0",
    "gatsby-transformer-sharp": "^4.21.0",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^1.4.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.18.5",
    "react-use": "^17.4.0",
    "typewriter-effect": "^2.19.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@a110/storybook-expand-all": "^1.1.1",
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-typescript": "^7.18.12",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-storysource": "^6.5.12",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/builder-webpack4": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack4": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/gatsbyjs__reach-router": "^1.3.0",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-i18next-extract": "^0.9.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-storybook": "^0.6.4",
    "lefthook": "^1.1.1",
    "prettier": "^2.7.1",
    "storybook-addon-gatsby": "^0.0.5",
    "storybook-dark-mode": "^1.1.2",
    "storybook-react-i18next": "^1.1.2",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ricardomfmsousa/my-vcard"
  }
}
